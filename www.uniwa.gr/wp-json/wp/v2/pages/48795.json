1 \u03ba\u03b1\u03c4\u03ac\u03c1\u03c4\u03b9\u03c3\u03b7, \u03b5\u03c0\u03b9\u03bc\u03cc\u03c1\u03c6\u03c9\u03c3\u03b7, \u03ba\u03b1\u03b9 \u03b5\u03be\u03b5\u03b9\u03b4\u03af\u03ba\u03b5\u03c5\u03c3\u03b7 \u03c0\u03bf\u03c5 \u03c5\u03c0\u03ac\u03c1\u03c7\u03bf\u03c5\u03bd \u03c3\u03c4\u03b7\u03bd \u03b5\u03bb\u03bb\u03b7\u03bd\u03b9\u03ba\u03ae \u03ba\u03bf\u03b9\u03bd\u03c9\u03bd\u03af\u03b1 \u03ba\u03b1\u03b9 \u03bf\u03b9\u03ba\u03bf\u03bd\u03bf\u03bc\u03af\u03b1, \u03b4\u03af\u03bd\u03b5\u03b9 \u03b9\u03b4\u03b9\u03b1\u03af\u03c4\u03b5\u03c1\u03b7 \u03b2\u03b1\u03c1\u03cd\u03c4\u03b7\u03c4\u03b1 \u03c3\u03c4\u03b7\u03bd \u03c5\u03bb\u03bf\u03c0\u03bf\u03af\u03b7\u03c3\u03b7 \u03c0\u03c1\u03bf\u03b3\u03c1\u03b1\u03bc\u03bc\u03ac\u03c4\u03c9\u03bd \u0394\u03b9\u03b1 \u0392\u03af\u03bf\u03c5 \u039c\u03ac\u03b8\u03b7\u03c3\u03b7\u03c2.<\/span><\/strong><\/p>\n<p style=\"text-align: left;\"><span style=\"font-size: 10pt;\">\u03a3\u03c4\u03b1 \u03c0\u03bb\u03b1\u03af\u03c3\u03b9\u03b1 \u03c4\u03b7\u03c2 \u03c0\u03b1\u03c1\u03b1\u03c0\u03ac\u03bd\u03c9 \u03c0\u03c1\u03bf\u03c3\u03ad\u03b3\u03b3\u03b9\u03c3\u03b7\u03c2,<\/span><\/p>\n<p> &#8230;<\/p><\/div>\n<p>  <a href=\"https:\/\/www.uniwa.gr\/spoydes\/dia-vioy-mathisi\/\" class=\"read_more\" target=\"_blank\">\u03a0\u03b5\u03c1\u03b9\u03c3\u03c3\u03cc\u03c4\u03b5\u03c1\u03b1<\/a><\/p>\n","protected":false},"author":11,"featured_media":0,"parent":48738,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"","meta":{"_acf_changed":false,"footnotes":""},"class_list":["post-48795","page","type-page","status-publish","hentry"],"acf":[],"aioseo_notices":[],"lang":"el","translations":{"el":48795,"en":42363},"publishpress_future_action":{"enabled":false,"date":"2025-04-22 19:52:11","action":"change-status","newStatus":"draft","terms":[],"taxonomy":"language"},"publishpress_future_workflow_manual_trigger":{"enabledWorkflows":[]},"pll_sync_post":[],"_links":{"self":[{"href":"https:\/\/www.uniwa.gr\/wp-js